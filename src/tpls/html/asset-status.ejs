        .bee-row-1 .bee-col-2 .bee-block-1 .bee-icon::not(.bee-icon-last) .bee-content {
            margin-right: 0
        }

        .bee-row-8 .bee-col-1 .bee-block-1 .bee-icon-image {
            padding: 5px 6px 5px 5px
        }

        .bee-row-8 .bee-col-1 .bee-block-1 .bee-icon::not(.bee-icon-last) .bee-content {
            margin-right: 0
        }

        .bee-row-3 .bee-col-1 .bee-block-1 {
            color: #f6f6f6;
            font-size: 20px;
            line-height: 120%;
            text-align: center
        }

        .bee-row-3 .bee-col-1 .bee-block-1 p:not(:last-child) {
            margin-bottom: 16px
        }

        .bee-row-5 .bee-col-1 .bee-block-2 {
            color: #101112;
            font-size: 12px;
            line-height: 150%;
            text-align: left
        }

        .bee-row-5 .bee-col-1 .bee-block-2 ul {
            list-style-type: revert;
            list-style-position: inside
        }

        .bee-row-5 .bee-col-1 .bee-block-2 li:not(:last-child) {
            margin-bottom: 0
        }

        .bee-row-5 .bee-col-1 .bee-block-2 li ul {
            margin-top: 0
        }

        .bee-row-5 .bee-col-1 .bee-block-2 li a {
            color: #8a3c90
        }

        .bee-row-5 .bee-col-1 .bee-block-2 li li {
            margin-left: 30px
        }

        @media (max-width: 585px) {
            .bee-row-content:not(.no_stack) {
                display: block
            }

            .bee-row-1 .bee-col-1 {
                padding: 0 !important
            }

            .bee-row-6 .bee-col-1 {
                padding: 5px 0 !important
            }

            .bee-row-1 .bee-col-2 .bee-block-1 .bee-icon-label {
                font-size: 14px !important;
                text-align: center
            }

            .bee-row-2 .bee-col-1 .bee-block-1 h1 {
