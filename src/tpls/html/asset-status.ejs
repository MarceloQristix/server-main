    <style>
        .bee-row,
        .bee-row-content {
            position: relative
        }

        .bee-list ul {
            margin: 0;
            padding: 0
        }

        .bee-row-1,
        .bee-row-1 .bee-row-content,
        .bee-row-2,
        .bee-row-3,
        .bee-row-4,
        .bee-row-5,
        .bee-row-6 {
            background-color: #000;
            background-repeat: no-repeat
        }

        .bee-icon .bee-icon-label-top {
            text-align: center
        }

        body {
            background-color: transparent;
            color: #000;
            font-family: Poppins, Arial, Helvetica, sans-serif
        }

        .bee-row-3 .bee-col-1 .bee-block-1 a,
        a {
            color: #fff
        }

        * {
            box-sizing: border-box
        }

        body,
        h1,
        p {
            margin: 0
        }

        .bee-row-content {
            max-width: 565px;
            margin: 0 auto;
            display: flex
        }

        .bee-row-content .bee-col-w3 {
            flex-basis: 25%
        }

        .bee-row-content .bee-col-w5 {
            flex-basis: 42%
        }

        .bee-row-content .bee-col-w7 {
            flex-basis: 58%
        }

        .bee-row-content .bee-col-w9 {
            flex-basis: 75%
        }

        .bee-row-content .bee-col-w12 {
            flex-basis: 100%
        }

        .bee-icon .bee-icon-label-right a,
        .bee-icon .bee-icon-label-top a {
            text-decoration: none
        }

        .bee-divider,
        .bee-image {
            overflow: auto
        }

        .bee-divider .center,
        .bee-image .bee-center {
            margin: 0 auto
        }

        .bee-row-2 .bee-col-2 .bee-block-1,
        .bee-row-3 .bee-col-1 .bee-block-3 {
            width: 100%
        }

        .bee-icon {
            display: inline-block;
            vertical-align: middle
        }

        .bee-icon .bee-content {
            display: flex;
            align-items: center
        }

        .bee-image img {
            display: block;
            width: 100%
        }

        .bee-paragraph,
        .bee-text {
            overflow-wrap: anywhere
        }

        .bee-row-1 .bee-row-content {
            border-radius: 0;
            color: #000
        }

        .bee-row-1 .bee-col-1 {
            padding-top: 5px
        }

        .bee-row-1 .bee-col-1 .bee-block-1,
        .bee-row-2 .bee-col-1 .bee-block-1 {
            padding: 10px;
            text-align: center;
            width: 100%
        }

        .bee-row-1 .bee-col-2,
        .bee-row-2 .bee-col-2,
        .bee-row-4 .bee-col-1,
        .bee-row-5 .bee-col-1,
        .bee-row-6 .bee-col-1,
        .bee-row-7 .bee-col-1,
        .bee-row-8 .bee-col-1 {
            padding-bottom: 5px;
            padding-top: 5px
        }

        .bee-row-1 .bee-col-2 .bee-block-1 {
            color: #fff;
