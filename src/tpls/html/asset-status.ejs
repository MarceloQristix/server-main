<div class="bee-page-container">
    <div class="bee-row bee-row-1">
        <div class="bee-row-content no_stack">
            <div class="bee-col bee-col-1 bee-col-w7">
                <div class="bee-block bee-block-1 bee-heading">
                    <h1 style="color:#4a3499;direction:ltr;font-family:'Montserrat', 'Trebuchet MS', 'Lucida Grande', 'Lucida Sans Unicode', 'Lucida Sans', Tahoma, sans-serif;font-size:20px;font-weight:700;letter-spacing:normal;line-height:150%;text-align:left;margin-top:0;margin-bottom:0;">
                        <span style="color: #ffffff;">BOOKING STATUS TRACKER</span><br/></h1>
                </div>
            </div>
            <div class="bee-col bee-col-2 bee-col-w5">
                <div class="bee-block bee-block-1 bee-icons">
                    <div class="bee-icon bee-icon-last">
                        <div class="bee-icon-label bee-icon-label-top">Menu</div>
                        <div class="bee-icon-image">
                            <a href="<%= asset.publicUrl %>?no-redirect=true">
                                <img alt="Menu" height="64px" src="<%= imagesBaseUrl %>/main-menu_1.png" width="auto"/>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="bee-row bee-row-2">
        <div class="bee-row-content no_stack">
            <div class="bee-col bee-col-1 bee-col-w9">
                <div class="bee-block bee-block-1 bee-heading">
                    <h1 style="color:#000000;direction:ltr;font-family:Poppins, Arial, Helvetica, sans-serif;font-size:18px;font-weight:400;letter-spacing:normal;line-height:150%;text-align:center;margin-top:0;margin-bottom:0;">
                        <strong>WELCOME TO THE WORLD OF </strong></h1>
                </div>
            </div>
            <div class="bee-col bee-col-2 bee-col-w3">
                <div class="bee-block bee-block-1 bee-image"><img alt="" class="bee-fixedwidth"
                                                                  src="<%= imagesBaseUrl %>/awasome.png"
                                                                  style="max-width:92px;"/></div>
            </div>
        </div>
    </div>
    <div class="bee-row bee-row-3">
        <div class="bee-row-content">
            <div class="bee-col bee-col-1 bee-col-w12">
                <div class="bee-block bee-block-1 bee-paragraph">
                    <p>
                        <span style="color: #000000;"><strong>Hello  </strong><strong><%= customer.title %> <%= customer.firstName %></strong></span>
                    </p>
                </div>
                <div class="bee-block bee-block-2 bee-divider">
                    <div class="center bee-separator" style="border-top:4px solid #F70A0A;width:100%;"></div>
                </div>
                <div class="bee-block bee-block-3 bee-image"><img alt="Image" class="bee-center bee-fixedwidth"
                                                                  src="<%= sku.primaryImage %>"
                                                                  style="max-width:339px;"/></div>
                <div class="bee-block bee-block-4 bee-divider">
                    <div class="center bee-separator" style="border-top:4px solid #F70A0A;width:100%;"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="bee-row bee-row-4">
        <div class="bee-row-content">
            <div class="bee-col bee-col-1 bee-col-w12">
                <div class="bee-block bee-block-1 bee-text">
                    <div class="bee-text-content"
                         style="font-size: 14px; line-height: 120%; font-family: 'Montserrat', 'Trebuchet MS', 'Lucida Grande', 'Lucida Sans Unicode', 'Lucida Sans', Tahoma, sans-serif; color: #000000;">
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"><span
                                    style="color: #ffffff; line-height: 16px; font-size: 14px;"><span
                                        style="line-height: 14px;"><span
                                            style="line-height: 14px;">Booking ID : <%= asset.serialNumber %>  | Booked On : <%= bookingDate %></span></span></span>
                        </p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"> </p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"><span
                                    style="color: #ffffff; line-height: 16px; font-size: 14px;"><span
                                        style="line-height: 14px;"><span
                                            style="line-height: 14px;">Vehicle : <%= sku.name %></span></span></span>
                        </p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"><span
                                    style="color: #ffffff; line-height: 16px; font-size: 14px;"><span
                                        style="line-height: 14px;"><span
                                            style="line-height: 14px;"> </span></span></span></p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"><span
                                    style="color: #ffffff; line-height: 16px; font-size: 14px;"><span
                                        style="line-height: 14px;"><span style="line-height: 14px;"><span
                                                style="line-height: 14px;">Dealership : <%= orgUnit.name %></span></span></span></span>
                        </p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"> </p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"><span
                                    style="color: #ffffff; line-height: 16px; font-size: 14px;"><span
                                        style="line-height: 14px;"><span style="line-height: 14px;">Tentative Delivery Quoted : <%= asset.custom?.tentativeDeliveryQuoted %></span></span></span>
                        </p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"> </p>
                        <p style="line-height: 16px; font-size: 14px;"><span
                                    style="color: #ffffff; line-height: 16px; font-size: 14px;">Quoted  Waiting  Period :
                                <%=asset.custom?.minWaitingPeriodInWeeksQuoted%> -
                                <%=asset.custom?.maxWaitingPeriodInWeeksQuoted%> Weeks  </span>
                        </p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"> </p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"><strong style=""><span
                                        style="font-size: 14px; color: #000000; line-height: 16px; background-color: #ffc200;"><span
                                            style="line-height: 14px;">Waiting Period Left as of <%=today%>  :
                                        <%=waitingPeriod %> </span></span></strong>
                        </p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"> </p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;">
                            <strong style="">

			<span style="background-color: #ffc200; color: #000000; line-height: 14px;">
				<span style="font-size: 14px; line-height: 16px; background-color: #ffc200;"><span
                            style="line-height: 14px;">Initial Booking Sequence No :  <%= asset.custom?.initialBookingSequenceNumber %> </span>
					<% if (asset.custom?.currentBookingSequenceNumber) {%>
                    <span>
                        <span style="line-height: 14px;">	| Current </span>
                    </span>
                    <span style="font-size: 14px; line-height: 16px; background-color: #ffc200;">
                        <span style="line-height: 14px;">Booking Sequence No  : <%= asset.custom?.currentBookingSequenceNumber %></span>
                    </span>
                    <%}%>
                </span>
            </span>
                            </strong>
                        </p>
                        <p style="font-size: 14px; line-height: 16px; text-align: left;"> </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
